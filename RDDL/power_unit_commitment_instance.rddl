non-fluents power_gen3 {
    domain = power_gen;

    objects {
		plant : {p1, p2, p3};
	};
}

// Specify three power plants with default settings
instance inst_power_gen {

	domain = power_gen;

	non-fluents = power_gen3;

	//objects {
	//	plant : {p1, p2, p3};
	//};
  
	init-state { 
		temperature = 10;
	};
  
  	// State-action constraints above are sufficient
	max-nondef-actions = pos-inf;
  
	horizon  = 40;
	discount = 1.0;
}